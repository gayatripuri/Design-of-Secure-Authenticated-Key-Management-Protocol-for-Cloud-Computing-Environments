        <div class="contact-page">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="inner-content"><br>
                            <center><h2>User Register</h2></center><br><br>
                            <div class="row">
                                <div class="col-lg-12 align-self-center">
                                    <form id="contact" action="userReg" method="post">
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <fieldset>
                                                    <label>Name :</label>
                                                    <input type="text" name="username" id="name" placeholder="Enter Your Name" required="">
                                                </fieldset>
                                            </div>
                                            <div class="col-lg-6">
                                                <fieldset>
                                                    <label>Email :</label>
                                                    <input type="email" name="email" id="name" placeholder="Enter Your Email" required="">
                                                </fieldset>
                                            </div>
                                            <div class="col-lg-6">
                                                <fieldset>
                                                    <label>DOB :</label>
                                                    <input type="date" name="dob" id="name" required="">
                                                </fieldset>
                                            </div>
                                            <div class="col-lg-6">
                                                <fieldset>
                                                    <label>Gender :</label>
                                                    <select name="gender" required="">
                                                        <option value="">Select Your Gender</option>
                                                        <option>Male</option>
                                                        <option>Female</option>
                                                        <option>Others</option>
                                                    </select>
                                                </fieldset>
                                            </div>
                                            <div class="col-lg-6">
                                                <fieldset>
                                                    <label>Phone No :</label>
                                                    <input type="text" name="phone" id="name" placeholder="Enter Your Phone No" required="">
                                                </fieldset>
                                            </div>
                                            <div class="col-lg-6">
                                                <fieldset>
                                                    <label>Address :</label>
                                                    <input type="text" name="address" id="name" placeholder="Enter Your Address" required="">
                                                </fieldset>
                                            </div>
                                            <div class="col-lg-6">
                                                <fieldset>
                                                    <label>Password :</label>
                                                    <input type="password" name="pass" id="email" placeholder="Enter Your Password" required="">
                                                </fieldset>
                                            </div>
                                            <div class="col-lg-12">
                                                <fieldset>
                                                    <center><button type="submit" id="form-submit" class="main-button "><i class="fa fa-sign-in"></i> Sign Up</button></center><br><br><br>
                                                </fieldset>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
